{
    "name": "beastbytes/icalendar",
    "type": "library",
    "description": "Create, edit, and import iCalendar",
    "keywords": [
        "icalendar",
        "RFC 5545",
        "RFC 7986"
    ],
    "license": "BSD-3-Clause",
    "require": {
        "php": "^8.0",
        "ext-mbstring": "*"
    },
    "require-dev": {
        "consolidation/robo": "^4.0",
        "phpunit/phpunit": "^10.0",
        "roave/infection-static-analysis-plugin": "^1.0",
        "vimeo/psalm": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "BeastBytes\\ICalendar\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BeastBytes\\ICalendar\\Tests\\": "tests"
        }
    },
    "scripts": {
        "infection": "roave-infection-static-analysis-plugin --log-verbosity=all --debug",
        "psalm": "psalm",
        "robo": "robo",
        "test": "phpunit --testdox"
    }
}
